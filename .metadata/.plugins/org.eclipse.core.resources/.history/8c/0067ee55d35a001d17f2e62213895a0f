package client;

public class RunMain {
	public static void main(String args[]) {
		ClientGuiView view = new ClientGuiView();
		
        java.awt.EventQueue.invokeLater(new Runnable() {
            @Override
            public void run() {
                new Main().setVisible(true);
            }
        });
		try {
			new Controller(view);
		} catch (Exception e) {
			// TODO Auto-generated catch block
			e.printStackTrace();
		}
	}
}
